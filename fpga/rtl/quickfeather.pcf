// camera clock
set_io CCLKO        33
set_io PCLK         32

set_io VSYNCI       31
set_io HREFI        55
// camera data
set_io CAM_DAT(7)   57
set_io CAM_DAT(6)   56
set_io CAM_DAT(5)   3
set_io CAM_DAT(4)   64
set_io CAM_DAT(3)   63
set_io CAM_DAT(2)   61
set_io CAM_DAT(1)   59
set_io CAM_DAT(0)   2
// SPI SRAM
set_io QUAD_CLK     23
set_io QUAD_SIO(0)  25
set_io QUAD_nCE     27

